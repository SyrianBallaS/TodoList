@{
    ViewBag.Title = "Todo List page";
}

<div class="jumbotron">
    <h1>Todo List</h1>
</div>

<div class="row">
    <div class="col-md-4">        
        <table>
            <tr><th>Todo List Items</th><th></th></tr>
            @foreach (var todo in Model)
            {
                <tr>
                    <td>@todo.Title </td>
                </tr>
            }
        </table>
    </div>
</div>
